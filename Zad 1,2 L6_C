def Ack(m, n):
	if (m == 0): return n + 1;
	if (m > 0 and n == 0): return Ack(m -1, 1);
	if (m > 0 and n > 0): return Ack(m - 1, Ack(m, n - 1));
'''	
#Zad 2
def nwd(a, b):
	if (b == 0 and a != 0): return a;
	if (a == 0 and b != 0): return b;
	if (b > 0 and a > 0): return nwd(b, a % b);

def main(n):
	'''liczby = [randint(1, 50) for _ in range(n)]
