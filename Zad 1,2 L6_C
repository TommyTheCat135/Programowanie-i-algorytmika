def nwd_iter(lista):
	x = nwd(lista[0], lista[1])
	for i in lista[2:]:
		x = nwd(x, i)
	return x

n = int(input("n = "))
lista = [randint(1, 100) for _ in range(n)]
print(nwd_iter(lista))

	
